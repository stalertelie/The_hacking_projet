<Agent>
  <Version>1.0k</Version>
  <IeVersion>11.674</IeVersion>
  <TemplateID>0</TemplateID>
  <AgentDefinition>

    <!--- - - - - - - - Browser Proxy Bypass Expressions - - - - - - - - -->

    <BrowserProxyBypassExpressionList>
      <BrowserProxyBypassExpression>regex:(?i)(mozenda\\.com|doubleclick\\.net|sitescout\\.com|moatads\\.com|mookie\\.com|adrta\\.com|adsafeprotected\\.com|mookie\\.com)</BrowserProxyBypassExpression>
      <BrowserProxyBypassExpression>regex:(?i)(\\.css|\\.js|\\.font)$</BrowserProxyBypassExpression>
      <BrowserProxyBypassExpression>regex:(?i)(\\.)(bitmap|bmp|gif|ico|icon|jpe|jpeg|jpg|mng|ora|pix|png|psd|svg|tga|tiff?)(\\?|$)</BrowserProxyBypassExpression>
      <BrowserProxyBypassExpression>regex:(?i)(\\.mpg|\\.mpeg|\\.swf|\\.flv|\\.ogg|\\.webm|\\.mp4|\\.mp3|\\.wav)$</BrowserProxyBypassExpression>
      <BrowserProxyBypassExpression>regex:(?i)(gstatic\\.com|googleapis\\.com|googlesyndication\\.com|google-analytics\\.com)</BrowserProxyBypassExpression>
    </BrowserProxyBypassExpressionList>
    <!--- - - - - - - - Actions - - - - - - - - -->

    <ActionList>

      <Action>
        <ActionID>Action-0</ActionID>
        <ActionType>LoadPage</ActionType>
        <Page>0</Page>
        <DestinationPage>1</DestinationPage>
        <TopLevelNav>True</TopLevelNav>
        <TopLevelNavCount>1</TopLevelNavCount>
        <PageUrl>http://mon-incubateur.com/site_incubateur/incubateurs</PageUrl>
      </Action>

      <!-- Page 1 -->

      <Action>
        <ActionID>Action-1</ActionID>
        <ActionType>PagePlaceHolder</ActionType>
        <Page>1</Page>
        <LoadUrl>http://mon-incubateur.com/site_incubateur/incubateurs</LoadUrl>
        <BaseUrl>http://mon-incubateur.com/site_incubateur/incubateurs</BaseUrl>
        <CaptureScreenShot>False</CaptureScreenShot>
      </Action>

      <Action>
        <ActionID>Action-2</ActionID>
        <ActionType>BeginAnchorList</ActionType>
        <Page>1</Page>
        <ItemType>PlaceHolder</ItemType>
        <ItemXPath>//*[@id=&amp;quot;incubators_list&amp;quot;]//tbody[3]/tr/td[2]/p[1]/span[1]/a[1]</ItemXPath>
        <ItemXPath>/html[1]/body[1]/div[3]/div[2]/div[1]/div[2]/table[1]/tbody[3]/tr/td[2]/p[1]/span[1]/a[1]</ItemXPath>
        <ListName>IncubateurList</ListName>
        <PreloadActionNodes>True</PreloadActionNodes>
      </Action>

      <Action>
        <ActionID>Action-4</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Description>Captures the text of the list item</Description>
        <Page>1</Page>
        <FieldExpression>%Incubateur%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <ItemXPath>.</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-5</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Page>1</Page>
        <FieldExpression>%ZIP%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <ItemXPath>../following-sibling::a[1]</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-6</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Page>1</Page>
        <FieldExpression> - %CITY%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <ItemXPath>../../..</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-8</ActionID>
        <ActionType>ClickElement</ActionType>
        <Page>1</Page>
        <DestinationPage>2</DestinationPage>
        <TopLevelNav>True</TopLevelNav>
        <TopLevelNavCount>1</TopLevelNavCount>
        <ItemType>PlaceHolder</ItemType>
        <ItemXPath>.</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-3</ActionID>
        <ActionType>EndList</ActionType>
        <Page>1</Page>
        <ListName>IncubateurList</ListName>
      </Action>

      <Action>
        <ActionID>Action-7</ActionID>
        <ActionType>PageList</ActionType>
        <Description>Clicks a link named &amp;#39;Suivant &amp;gt;&amp;#39; to page the list</Description>
        <Page>1</Page>
        <IsOptional>True</IsOptional>
        <Value>Suivant &amp;gt;</Value>
        <WaitAjax>2</WaitAjax>
        <CatchNavToSamePage>True</CatchNavToSamePage>
        <ItemType>HyperLink</ItemType>
        <AssociatedTextXPath>//tbody[4]/tr[1]/td[1]/p[1]/a[normalize-space(.)=&amp;#39;Suivant &amp;gt;&amp;#39;]</AssociatedTextXPath>
        <ItemXPath>.</ItemXPath>
        <DedupeAnchorNodes>False</DedupeAnchorNodes>
        <ListName>IncubateurList</ListName>
      </Action>

      <!-- Page 2 -->

      <Action>
        <ActionID>Action-9</ActionID>
        <ActionType>PagePlaceHolder</ActionType>
        <Page>2</Page>
        <FieldExpression>PageUrl:%URL%</FieldExpression>
        <WaitAjax>2</WaitAjax>
        <LoadUrl>http://mon-incubateur.com/site_incubateur/incubateur/axeleo-charbonnieres-les-bains</LoadUrl>
        <BaseUrl>http://mon-incubateur.com/site_incubateur/incubateur/axeleo-charbonnieres-les-bains</BaseUrl>
        <CaptureScreenShot>False</CaptureScreenShot>
      </Action>

      <Action>
        <ActionID>Action-10</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Page>2</Page>
        <IsOptional>True</IsOptional>
        <FieldExpression>%dureeIncuation%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <AssociatedTextXPath>//td[normalize-space(.)=&amp;#39;Type&amp;#39;]</AssociatedTextXPath>
        <ItemXPath>../following-sibling::tr[1]/td[2]</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-11</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Page>2</Page>
        <IsOptional>True</IsOptional>
        <FieldExpression>%cout%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <AssociatedTextXPath>//td[normalize-space(.)=&amp;quot;Duree de l&amp;#39;incubation&amp;quot;]</AssociatedTextXPath>
        <ItemXPath>../following-sibling::tr[1]/td[2]</ItemXPath>
      </Action>

      <Action>
        <ActionID>Action-12</ActionID>
        <ActionType>GetElementValue</ActionType>
        <Page>2</Page>
        <IsOptional>True</IsOptional>
        <FieldExpression>%nombre de places par an%</FieldExpression>
        <ItemType>PlaceHolder</ItemType>
        <AssociatedTextXPath>//td[normalize-space(.)=&amp;#39;Nombre de places par an&amp;#39;]</AssociatedTextXPath>
        <ItemXPath>following-sibling::td[1]</ItemXPath>
      </Action>

    </ActionList>

  </AgentDefinition>
</Agent>